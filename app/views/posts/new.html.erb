<h1>New Post</h1>
<button><%= link_to "Home", root_path %></button>
<br />
<br />
<%= form_for @post do |f| %>
	<%= f.label :author %>
	<%= f.text_field :author %>
	<br />
	<br />
	<%= f.label :title %>
	<%= f.text_field :title %>
  <br />
  <br />
	<%= f.label :body %>
	<%= f.text_area :body %>
	<br />
	<%= f.submit %>
<% end %>